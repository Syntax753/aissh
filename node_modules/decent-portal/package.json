{
  "name": "decent-portal",
  "description": "Decent Portal Library",
  "main": "dist/decent-portal.umd.cjs",
  "module": "dist/decent-portal.js",
  "types": "dist/decent-portal.d.ts",
  "private": false,
  "license": "MIT",
  "author": "Erik Hermansen",
  "version": "2.4.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc && mv dist/index.d.ts dist/decent-portal.d.ts && rm dist/*.png",
    "pack": "npm run build && npm pack",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "update-model-list": "node --experimental-transform-types scripts/generateModelList.ts > src/models/modelList.json"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@webgpu/types": "^0.1.64",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "terser": "^5.43.1",
    "vite": "^7.0.6",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist",
    "github-workflows",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "sideEffects": false,
  "dependencies": {
    "typescript": "^5.8.3"
  }
}
